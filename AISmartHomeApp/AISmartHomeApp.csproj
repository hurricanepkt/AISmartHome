<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>net7.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <PublishProfile>DefaultContainer</PublishProfile>
    <RollForward>LatestMinor</RollForward>
    <ContainerImageTags>1.1.0;latest</ContainerImageTags>  
    <RuntimeIdentifier>linux-x64</RuntimeIdentifier>
    <ContainerPort>80</ContainerPort>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="AisParser" Version="1.0.76" />
    <PackageReference Include="FileBaseContext" Version="1.0.1" />
    <PackageReference Include="MediatR" Version="12.0.1" />
    <PackageReference Include="Microsoft.EntityFrameworkCore" Version="7.0.4" />
    <PackageReference Include="Microsoft.NET.Build.Containers" Version="0.4.0" />
    <PackageReference Include="Swashbuckle.AspNetCore" Version="6.5.0" />
    <PackageReference Include="Swashbuckle.AspNetCore.SwaggerUI" Version="6.5.0" />
  </ItemGroup>

</Project>
